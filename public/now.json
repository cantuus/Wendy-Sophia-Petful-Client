{
    "version": 2,
    "name": "petful",
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "^/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "^/manifest.json",
            "dest": "/manifest.json"
        },
        {
            "src": ".*",
            "dest": "/index.html"
        }
    ]
}